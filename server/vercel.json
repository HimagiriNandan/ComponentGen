{
    "version": 2,
    "builds": [
      { "src": "index.js", "use": "@vercel/node" }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "headers": {
          "Access-Control-Allow-Origin": "https://component-gen.vercel.app/",
          "Access-Control-Allow-Credentials": "true"
        },
        "dest": "index.js"
      }
    ]
  }